<html lang="en" class="no-js">
<!--<![endif]-->
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <title><g:layoutTitle default="Officegain"/></title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="shortcut icon" href="${resource(dir: 'images', file: 'favicon.ico')}" type="image/x-icon">
    <link rel="apple-touch-icon" href="${resource(dir: 'images', file: 'apple-touch-icon.png')}">
    <link rel="apple-touch-icon" sizes="114x114" href="${resource(dir: 'images', file: 'apple-touch-icon-retina.png')}">
    <link rel="stylesheet" href="${resource(dir: 'css', file: 'main.css')}" type="text/css">
    <link rel="stylesheet" href="${resource(dir: 'css', file: 'menu.css')}" type="text/css">
    <link rel="stylesheet" href="${resource(dir: 'css', file: 'mobile.css')}" type="text/css">
    <resource:treeView/>
    <g:javascript library="jquery"/>
    <r:require module="jquery-ui"/>
    <r:layoutResources/>
    <g:layoutHead/>
</head>

<body>
<div id="officegainLogo" role="banner"><a href="${request.contextPath}/">
    <img src="${resource(dir: 'images', file: 'officegain_logo.png')}" alt="Officegain"/></a>

    <img src="${resource(dir: 'images', file: 'lock.png')}" alt=""
         style="margin-left: 200px;"/>&nbsp;You are logged in as <span><sec:username/></span>
    <i><a href="${request.contextPath}/logout" style="margin-left: 25px">Logout</a></i>

</div>

<div class="nav" role="navigation">
    <ul class="myMenu">
        <li><a class="home" href="${request.contextPath}/">Category</a></li>
        <li><a class="home" href="${request.contextPath}/manufacturer">Manufacturer</a></li>
        <li><a class="home" href="${request.contextPath}/product">Product</a></li>
        <li><g:form method="get" controller="AdminSearch"
                    style="display: table; margin-left: 332px; max-width: 300px; text-align: left;">
            <richui:autoComplete name="q"
                                 action="${createLinkTo('dir': 'AdminSearch/searchAJAX')}"/>
            <div style="display:table-cell;"><g:submitButton name="search"/></div>
        </g:form></li>
    </ul>
</div>
<g:layoutBody/>
<div class="footer" role="contentinfo">
    <div><span
            style=" margin-left: 300px ;marquee-speed: slow; ">Â© Copyright 2012, all rights reserved with Officegain</span>
    </div>
</div>

<div id="spinner" class="spinner" style="display:none;"><g:message code="spinner.alt"
                                                                   default="Loading&hellip;"/></div>
</body>
</html>