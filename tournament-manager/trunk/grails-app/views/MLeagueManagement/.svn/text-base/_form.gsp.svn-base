<%@ page import="com.maali.tournamentmanager.MLeagueManagement" %>



<div class="fieldcontain ${hasErrors(bean: MLeagueManagementInstance, field: 'leagueManagementCol', 'error')} required">
	<label for="leagueManagementCol">
		<g:message code="MLeagueManagement.leagueManagementCol.label" default="League Management Col" />
		<span class="required-indicator">*</span>
	</label>
	<g:textField name="leagueManagementCol" required="" value="${MLeagueManagementInstance?.leagueManagementCol}"/>
</div>

<div class="fieldcontain ${hasErrors(bean: MLeagueManagementInstance, field: 'leagueManagementCol1', 'error')} required">
	<label for="leagueManagementCol1">
		<g:message code="MLeagueManagement.leagueManagementCol1.label" default="League Management Col1" />
		<span class="required-indicator">*</span>
	</label>
	<g:textField name="leagueManagementCol1" required="" value="${MLeagueManagementInstance?.leagueManagementCol1}"/>
</div>

<div class="fieldcontain ${hasErrors(bean: MLeagueManagementInstance, field: 'currentPosition', 'error')} ">
	<label for="currentPosition">
		<g:message code="MLeagueManagement.currentPosition.label" default="Current Position" />
		
	</label>
	<g:checkBox name="currentPosition" value="${MLeagueManagementInstance?.currentPosition}" />
</div>

<div class="fieldcontain ${hasErrors(bean: MLeagueManagementInstance, field: 'endDate', 'error')} required">
	<label for="endDate">
		<g:message code="MLeagueManagement.endDate.label" default="End Date" />
		<span class="required-indicator">*</span>
	</label>
	<g:datePicker name="endDate" precision="day"  value="${MLeagueManagementInstance?.endDate}"  />
</div>

<div class="fieldcontain ${hasErrors(bean: MLeagueManagementInstance, field: 'leagueId', 'error')} required">
	<label for="leagueId">
		<g:message code="MLeagueManagement.leagueId.label" default="League Id" />
		<span class="required-indicator">*</span>
	</label>
	<g:field type="number" name="leagueId" required="" value="${fieldValue(bean: MLeagueManagementInstance, field: 'leagueId')}"/>
</div>

<div class="fieldcontain ${hasErrors(bean: MLeagueManagementInstance, field: 'memberId', 'error')} ">
	<label for="memberId">
		<g:message code="MLeagueManagement.memberId.label" default="Member Id" />
		
	</label>
	<g:textField name="memberId" value="${MLeagueManagementInstance?.memberId}"/>
</div>

<div class="fieldcontain ${hasErrors(bean: MLeagueManagementInstance, field: 'memberRole', 'error')} ">
	<label for="memberRole">
		<g:message code="MLeagueManagement.memberRole.label" default="Member Role" />
		
	</label>
	<g:textField name="memberRole" value="${MLeagueManagementInstance?.memberRole}"/>
</div>

<div class="fieldcontain ${hasErrors(bean: MLeagueManagementInstance, field: 'startDate', 'error')} required">
	<label for="startDate">
		<g:message code="MLeagueManagement.startDate.label" default="Start Date" />
		<span class="required-indicator">*</span>
	</label>
	<g:datePicker name="startDate" precision="day"  value="${MLeagueManagementInstance?.startDate}"  />
</div>

