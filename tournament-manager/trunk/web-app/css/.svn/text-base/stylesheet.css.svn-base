@charset "utf-8";
/* CSS Document */
body {
    margin: 0px;
    padding: 0px;
    background-image: url(../images/back_ground_image.jpg);
    background-position: center top;
    background-repeat: no-repeat;
    background-color: #000000;
}

.maindiv {
    margin-left: 12%;
    margin-right: 12%;
    width: 66%;
    float: left;
    padding: 0px;
}

.header {
    width: 100%;
    margin: 0px;
    padding: 13px 0px 13px 0px;
    float: left;
    background-image: url(../images/trans_shadow.png);
    background-position: top left;
    background-repeat: repeat-x;
}

.header1 {
    width: 100%;
    margin: 0px;
    padding: 15px 0px 15px 0px;
    float: left;
    background-color: #416bd7;
    border-top: 1px solid #ffffff;
}

.headerdivone {
    margin: 0px;
    padding: 0px;
    float: right;
    text-align: right;
}

.headerdivtwo {
    margin: 0px;
    padding: 0px;
    float: right;
    text-align: right;
}

.body_area {
    width: 100%;
    margin: 0px;
    padding: 10px 0px 10px 0px;
}

.footerdiv {
    width: 100%;
    margin: 0px;
    padding: 10px 0px 10px 0px;
}

.logodiv {
    margin-top: -13px;
    margin-left: 220px;
    padding: 0px;
    position: absolute;
    z-index: 100;
    float: left;
}

.buttons:link, .buttons:visited {
    -moz-border-radius: 5px;
    border-radius: 5px;
    border: 1px solid #ffffff;
    background: -moz-linear-gradient(top, #ffffff, #eeeeee);
    background: -webkit-linear-gradient(top, #ffffff, #eeeeee);
    background: -o-linear-gradient(top, #ffffff, #eeeeee);
    background: linear-gradient(top, #ffffff, #eeeeee);
    padding: 5px 20px 5px 20px;
    font-family: "Calibri";
    font-size: 15px;
    color: #000000;
    font-weight: normal;
    text-transform: capitalize;
    text-decoration: none;
}

.buttons:hover {
    -moz-border-radius: 5px;
    border-radius: 5px;
    border: 1px solid #cfcfcf;
    background: -moz-linear-gradient(top, #e3e3e3, #ffffff);
    background: -webkit-linear-gradient(top, #e3e3e3, #ffffff);
    background: -o-linear-gradient(top, #e3e3e3, #ffffff);
    background: linear-gradient(top, #e3e3e3, #ffffff);
    padding: 5px 20px 5px 20px;
    font-family: "Calibri";
    font-size: 15px;
    color: #000000;
    font-weight: normal;
    text-transform: capitalize;
}

.bodyarea {
    width: 100%;
    margin: 0px;
    padding: 0px 0px 15px 0px;
    float: left;
    border-top: 1px solid #000000;
    background-image: url(../images/trans_shadow.png);
    background-position: top left;
    background-repeat: repeat-x;
}

.loginbox {
    float: right;
    margin-right: 19%;
    padding: 20px;
    background-image: -ms-linear-gradient(top, #FFFFFF 0%, #dddddd 100%);
    background-image: -moz-linear-gradient(top, #FFFFFF 0%, #dddddd 100%);
    background-image: -o-linear-gradient(top, #FFFFFF 0%, #dddddd 100%);
    background-image: -webkit-gradient(linear, left top, left bottom, color-stop(0, #FFFFFF), color-stop(1, #dddddd));
    background-image: -webkit-linear-gradient(top, #FFFFFF 0%, #dddddd 100%);
    background-image: linear-gradient(to bottom, #FFFFFF 0%, #dddddd 100%);
    border-left: 1px solid #000000;
    border-right: 1px solid #000000;
    border-bottom: 1px solid #000000;
    width: 35%;
}

.rowone {
    margin: 0px;
    padding: 0px;
    text-align: center;
    float: left;
    width: 100%;
    font-family: "BebasNeue";
    font-size: 29px;
    color: #000000;
}

.rowtwo {
    background-image: url(../images/form_devider.png);
    background-position: center top;
    background-repeat: no-repeat;
    padding: 20px 0px 20px 0px;
    margin-top: 10%;
    text-align: center;
}

.rowthree {
    padding: 0px 0px 15px 0px;
    margin: 0px;
    text-align: center;
}

.rowfour {
    padding: 0px 0px 15px 0px;
    margin: 0px;
    width: 100%;
}

.title {
    margin: px;
    padding-top: 5px;
    float: left;
    width: 30%;
    text-align: right;
    font-family: "BebasNeue";
    font-size: 21px;
    color: #7e7e7e;
    text-transform: uppercase;
    text-shadow: 1px 1px 1px #ffffff;
}

.textfield {
    margin-left: 10px;
    padding: 0px;
    float: left;
    width: 65%;
}

.inputfield {
    border: 1px solid #b7b7b7;
    padding: 2px 5px 2px 5px;
    background-color: #ffffff;
    font-family: "BebasNeue";
    font-size: 21px;
    color: #000000;
    width: 90%;
}

.paddingtop {
    padding-top: 30px;
}

.reg_button:link, .reg_button:visited {
    font-family: "BebasNeue";
    font-size: 1.4em;
    color: #ffffff;
    padding: 5px 35px 3px 35px;
    background-color: #5daf1f;
    text-decoration: none;
}

.reg_button:hover {
    font-family: "BebasNeue";
    font-size: 1.4em;
    color: #ffffff;
    padding: 5px 35px 3px 35px;
    background-color: #519f15;
    text-decoration: none;
}

.blksmall {
    font-family: Arial, Helvetica, sans-serif;
    font-size: 10px;
    color: #000000;
}

.footer {
    width: 100%;
    margin-top: 4%;
    padding: 10px 0px 15px 0px;
    float: left;
    background-color: #416bd7;
    border-top: 1px solid #ffffff;
}

.footerpanel {
    margin: 0px;
    padding: 0px;
    float: left;
    color: #ffffff;
    font-family: Arial, Helvetica, sans-serif;
    font-size: 11px;
    width: 19%;
    line-height: 20px;
}

.whitelink:link, .whitelink:visited {
    color: #ffffff;
    font-family: Arial, Helvetica, sans-serif;
    font-size: 11px;
    text-decoration: none;
}

.whitelink:hover {
    color: #ffffff;
    font-family: Arial, Helvetica, sans-serif;
    font-size: 11px;
    text-decoration: underline;
}

.footerpane2 {
    margin: 0px;
    padding: 0px;
    float: left;
    color: #ffffff;
    font-family: Arial, Helvetica, sans-serif;
    font-size: 11px;
    width: 24%;
    line-height: 20px;
}

.footerraw1 {
    width: 100%;
    padding: 0px;
    margin: 0px;
    float: left;
}

.footerinput {
    border: 1px solid #b7b7b7;
    padding: 2px 5px 2px 5px;
    background-color: #ffffff;
    font-family: Arial, Helvetica, sans-serif;
    font-size: 12px;
    color: #000000;
    width: 90%;
}

.padtb {
    padding: 10px 0px 10px 0px;
}

.endfooter {
    width: 100%;
    margin: 0px;
    padding: 10px 0px 15px 0px;
    float: left;
    background-image: url(../images/trans_shadow_blk.png);
    background-position: top left;
    background-repeat: repeat-x;
    background-color: #ffffff;
    border-top: 1px solid #ffffff;
    font-family: Arial, Helvetica, sans-serif;
    font-size: 11px;
    color: #666666;
}

/* MESSAGES AND ERRORS */

.errors,
.message {
    font-size: 0.8em;
    line-height: 2;
    margin: 1em 2em;
    padding: 0.5em;
}

.message {
    background: #f3f3ff;
    border: 1px solid #b2d1ff;
    color: #006dba;
    -moz-box-shadow: 0 0 0.25em #b2d1ff;
    -webkit-box-shadow: 0 0 0.25em #b2d1ff;
    box-shadow: 0 0 0.25em #b2d1ff;
}

.errors {
    background: #fff3f3;
    border: 1px solid #ffaaaa;
    color: #cc0000;
    -moz-box-shadow: 0 0 0.25em #ff8888;
    -webkit-box-shadow: 0 0 0.25em #ff8888;
    box-shadow: 0 0 0.25em #ff8888;
}

.errors ul,
.message {
    padding: 0;
}

.errors li {
    list-style: none;
    background: transparent url(../images/skin/exclamation.png) 0 50% no-repeat;
    text-indent: 22px;
}

.message {
    background: transparent url(../images/skin/information.png) 0 50% no-repeat;
    text-indent: 22px;
}

/* form fields with errors */

.error input, .error select, .error textarea {
    background: #fff3f3;
    border-color: #ffaaaa;
    color: #cc0000;
}

.error input:focus, .error select:focus, .error textarea:focus {
    -moz-box-shadow: 0 0 0.5em #ffaaaa;
    -webkit-box-shadow: 0 0 0.5em #ffaaaa;
    box-shadow: 0 0 0.5em #ffaaaa;
}

